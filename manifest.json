{
  "manifest_version": 2,
  "name": "Alt Helper",
  "description": "A sample chrome extension that demonstrates using Einstein Vision to  add alt text to images",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Alt Helper"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://einstenforgood.herokuapp.com/*"
  ]
}
